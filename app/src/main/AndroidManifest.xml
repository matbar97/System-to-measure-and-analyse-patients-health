<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.telm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AccountDoctorDetailsActivity"></activity>
        <activity android:name=".AccountDetailsPatientActivity" />
        <activity android:name=".PatientCardDataFromPatientPointOfView" />
        <activity android:name=".PatientsMenuActivity" />
        <activity android:name=".SearchForPatientActivity" />
        <activity android:name=".StudyReviewActivity" />
        <activity android:name=".AddStudyActivity" />
        <activity android:name=".PatientDataFromDocPointOfViewActivity" />
        <activity android:name=".PatientCreationActivity" />
        <activity android:name=".DoctorsMainMenuActivity" />
        <activity android:name=".CreatePatientAccountActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>